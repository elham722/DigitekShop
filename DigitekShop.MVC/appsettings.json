{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApiSettings": {
    "BaseUrl": "https://localhost:7223"
  },
  "LocalStorage": {
    "AutoLoad": true,
    "AutoSave": true,
    "Filename": "DigitekShop.LocalStorage",
    "Directory": "LocalStorage"
  },
  "ExternalService": {
    "BaseUrl": "https://localhost:7223",
    "TimeoutSeconds": 30,
    "UseHttps": true,
    "DefaultHeaders": {
      "Accept": "application/json"
    }
  },
  "ConnectionStrings": {
    "IdentityConnection": "Server=(localdb)\\mssqllocaldb;Database=DigitekShopIdentity;Trusted_Connection=true;MultipleActiveResultSets=true"
  }
}
